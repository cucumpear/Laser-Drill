<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <!--============================ Resource rocks ===============================-->

  
    <ThingDef ParentName="RockBase">
    <defName>MineableViosteel</defName>
    <label>compacted viosteel</label>
    <graphicData>
      <texPath>Things/Building/Linked/RockFlecked_Atlas</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <color>(91,50,86)</color>
      <colorTwo>(133,96,136)</colorTwo>
    </graphicData>
    <statBases>
      <MaxHitPoints>12000</MaxHitPoints>
    </statBases>
    <description>Ancient compacted rubble rich in viosteel.</description>
    <building>
      <isResourceRock>true</isResourceRock>
      <mineableThing>Viosteel</mineableThing>
      <mineableYield>35</mineableYield>
      <mineableScatterCommonality>0.05</mineableScatterCommonality>
      <mineableScatterLumpSizeRange>
        <min>5</min>
        <max>30</max>
      </mineableScatterLumpSizeRange>
    </building>
  </ThingDef>
  
