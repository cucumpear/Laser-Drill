    <fixedIngredientFilter>
      <thingDefs>
        <li>Viosteel</li>
        <li>DevilstrandCloth</li>
      </thingDefs>
    </fixedIngredientFilter>
	<researchPrerequisite>Refining</researchPrerequisite>
  </RecipeDef>

